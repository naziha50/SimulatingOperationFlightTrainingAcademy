<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="394.0" prefWidth="452.0" spacing="10.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx.controllers.MaterialsController">
    <Label prefHeight="65.0" prefWidth="173.0" style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Upload Materials" />
    <TextField promptText="Enter Material Name" />
    <HBox prefHeight="35.0" prefWidth="372.0" spacing="10.0">
        <Button prefHeight="24.0" prefWidth="439.0" text="Choose File"/>
        <Button onAction="#handleUpload" prefHeight="24.0" prefWidth="449.0" text="Upload"/>
    </HBox>
    <Button onAction="#goBack" prefHeight="24.0" prefWidth="108.0" styleClass="bg" text="Back" />
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
